{
  "name": "react_boilerplate",
  "version": "1.0.0",
  "description": "My React boilerplate using Parcel, Express, Babel, Eslint, Prettier, and more.",
  "main": "index.js",
  "scripts": {
    "start": "babel-node ./server/index.js",
    "build": "NODE_ENV=production npx parcel build src/index.html -d public/ --no-cache",
    "lint": "eslint . --ext .js,.jsx"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "react",
    "boilerplate",
    "parcel",
    "css modules",
    "express",
    "prettier",
    "eslint",
    "babel",
    "antd"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^3.2.3",
    "autoprefixer": "^8.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "opn": "^5.2.0",
    "parcel-bundler": "^1.6.2",
    "postcss-modules": "^1.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "uuid": "^3.2.1"
